<%- include('header')%>
   
<h2>
    Editar vaga : |
    <a href="/admin/vagas">Vagas</a>

</h2>
<form method="POST">

    <label>Titulo</label>
    <input type="text" name="titulo" value="<%=vaga.titulo %>"/><br>
    <label>Descrição</label>
    <textarea va name="descricao"><%=vaga.descricao %></textarea><br>
    <label>Categoria</label>
    <select name="categoria">
       <% categorias.forEach(categoria =>{ %>
        <option value="<%- categoria.id %>"><%- categoria.categoria %></option>
        <% }) %>
    </select><br>
    <button>Salvar vaga</button>
</form>
    
<div>

</div>


